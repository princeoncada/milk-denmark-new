<script>
    const a = document.querySelectorAll('.carousel__btn');
    a.forEach((el) => {
      el.addEventListener('click', (e) => {
        e.preventDefault();
        const target = el.getAttribute('href');
        const item = document.querySelector(target);
        const carousel = document.querySelector('.x-carousel');
        carousel.scrollLeft = item.offsetLeft;
      });
    });
</script>